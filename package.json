{
  "scripts": {
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd Keystone-master && npm run build",
    "build:backend": "cd Keystone-master/src/next && npm run build",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd Keystone-master && npm start",
    "start:backend": "cd Keystone-master/src/next && npm run env"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
