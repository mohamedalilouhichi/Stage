{
  "version": 2,
  "builds": [
    {
      "src": "Keystone-master/src/next/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "Keystone-master/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Keystone-master/src/next/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://your-api-url.com",
    "ANOTHER_ENV_VARIABLE": "value"
  },
  "functions": {
    "api/**/*": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
